{
  "name": "chronibuild",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "scraper/items": "nodemon --exec 'node --experimental-json-modules ./tools/items_scraper.js' --watch ./tools/items_scraper.js",
    "scraper/runes": "nodemon --exec 'node --experimental-json-modules ./tools/runes_scraper.js' --watch ./tools/runes_scraper.js"
  },
  "dependencies": {
    "@ll931217/vue-treeview": "../vue-treeview/master",
    "axios": "^0.20.0",
    "buefy": "^0.9.3",
    "core-js": "^3.6.5",
    "pug-plain-loader": "^1.0.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.21.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.4",
    "pug": "^3.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.4",
    "vue-cli-plugin-i18n": "^1.0.1",
    "vue-template-compiler": "^2.6.11"
  }
}
